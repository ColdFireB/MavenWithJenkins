import java.util.Scanner;

public class Numbering {

	// Scanner s = new Scanner(System.in);
	// int n = s.nextInt();

	static int n1 = 5;

	static int num = 0;

	public static void printNumbers() {

		for (int i = ((n1 * n1) + n1 - 1); i >= 1;) {
			System.out.print(i);
			System.out.print(" ");
			i = i - 2;
			num++;
			if (num == n1) {
				System.out.println();
				n1=n1-1;
			}
		}
	}

	public static void main(String[] args) {
		printNumbers();
	}

}

import java.util.Scanner;

public class Numbering {

	// Scanner s = new Scanner(System.in);
	// int n = s.nextInt();



	public void printNumbers() {
		 int n1 = 5;

		 int num = 0;
		for (int i = ((n1 * n1) + n1 - 1); i >= 1;) {
			System.out.print(i);
			System.out.print(" ");
			i = i - 2;
			num++;
			if (num == n1) {
				System.out.println();
				n1=n1-1;
				num = 0;
				System.out.println("count   " + n1);
			}
		}
	}

	public static void main(String[] args) {
		Numbering s = new Numbering();
		s.printNumbers();
	}

}
